{
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "proseWrap": "always",
    "endOfLine": "lf",
    "tabWidth": 2,
    "printWidth": 120,
    "overrides": [{
        "files": "*.md",
        "options": {
            "proseWrap": "preserve"
        }
    }]
}
